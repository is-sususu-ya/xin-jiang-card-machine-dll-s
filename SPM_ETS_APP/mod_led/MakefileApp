
include ../Makefile.param
#ifeq ($(SAMPLE_PARAM_FILE), )
#     SAMPLE_PARAM_FILE:=../Makefile.param
#     include $(SAMPLE_PARAM_FILE)
#endif
.PHONY: all lib clean

LIBSRC:=$(wildcard *.c)
LIBOBJ:=$(LIBSRC:%.c=%.o)
APP_FLAGS:= -lpthread -lrt -lm

# this will be set for each module
MLIB:=mod_led

lib: $(MLIB)
$(MLIB) : $(LIBOBJ)
	$(CC)  -o $@  $^ $(SYS_FLAGS) $(CLANE_LFLAGS)
	cp $(MLIB) /home/linux/Share_Smaba/01_Release/

	
$(LIBOBJ) : %.o:%.c
	$(CC) -DENABLE_TESTCODE $(INCLUDE_PATH) -c $^ -o $@

clean:
	rm -f $(LIBOBJ)
	rm -f $(MLIB)
	rm -f tags
